import pandas as pd
from sklearn.linear_model import LinearRegression

# Sample dataset
data = {
    "Hours_Studied": [1, 2, 3, 4, 5],
    "Scores": [50, 55, 65, 70, 80]
}

df = pd.DataFrame(data)

# Model
X = df[["Hours_Studied"]]
y = df["Scores"]

model = LinearRegression()
model.fit(X, y)

# Predict
pred_hours = 6
pred_score = model.predict([[pred_hours]])
print(f"Predicted score for {pred_hours} hours of study: {pred_score[0]:.2f}")
